{% extends 'BackOffice/baseback.html.twig' %}

{% block title %}New Tablee{% endblock %}

{% block body %}
    <h1>Create new Tablee</h1>
    
    <a href="{{ path('app_tablee_index') }}">
    <span style="margin-left: 90%">Back to list<span>
    <i class="fa fa-angle-right"></i>
  </a>
  {{form_start(form,{'attr': {'novalidate': 'novalidate'}} ) }}

   <div class="form-group">
    <label for="name">number</label>


      {{ form_widget(form.nbrPlace , {'attr' : {'class' : 'form-control'  }}  ) }}
                                
        {{ form_errors(form.nbrPlace ,  {'attr' : {'class' : 'text-danger'  }}) }}
  </div>
   <div class="form-group">
    <label for="name">Description</label>


      {{ form_widget(form.description , {'attr' : {'class' : 'form-control'  }}  ) }}
                                
        {{ form_errors(form.description ,  {'attr' : {'class' : 'text-danger'  }}) }}
  </div>
   <div class="form-group">
    <label for="name">Restaurant</label>


      {{ form_widget(form.id_Restau , {'attr' : {'class' : 'form-control'  }}  ) }}
                                
        {{ form_errors(form.id_Restau) }}
  </div>
<button class="btn btn-primary mb-2">{{ button_label|default('Save') }}</button>
 
  {{ form_end(form) }} 
     
{% endblock %}


   