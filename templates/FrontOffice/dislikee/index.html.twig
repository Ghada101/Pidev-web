{% extends 'base.html.twig' %}

{% block title %}Dislikee index{% endblock %}

{% block body %}
    <h1>Dislikee index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Dislikeid</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for dislikee in dislikees %}
            <tr>
                <td>{{ dislikee.dislikeid }}</td>
                <td>
                    <a href="{{ path('app_dislikee_show', {'dislikeid': dislikee.dislikeid}) }}">show</a>
                    <a href="{{ path('app_dislikee_edit', {'dislikeid': dislikee.dislikeid}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="2">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_dislikee_new') }}">Create new</a>
{% endblock %}
